{
	"name": "boilerplatebackendsetup",
	"version": "1.0.0",
	"description": "React App backend boilerplate project setup",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node backend/server",
		"server": "nodemon backend/server",
		"client": "npm start --prefix frontend",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node backend/seeder",
		"data:destroy": "node backend/seeder -d",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gamahsoft/ReactAppFrontBackEndBoilerPlate.git"
	},
	"keywords": [
		"ReactApp",
		"project",
		"setup",
		"frontend",
		"backend"
	],
	"author": "Rami Reddy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gamahsoft/ReactAppFrontBackEndBoilerPlate/issues"
	},
	"homepage": "https://github.com/gamahsoft/ReactAppFrontBackEndBoilerPlate#readme",
	"devDependencies": {
		"concurrently": "^6.4.0",
		"nodemon": "^2.0.15"
	},
	"dependencies": {
		"colors": "^1.4.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^5.5.1",
		"helmet": "^4.6.0",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.3",
		"xss-clean": "^0.1.1"
	}
}
